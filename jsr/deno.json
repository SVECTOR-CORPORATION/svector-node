{
  "name": "@svector/svector",
  "version": "1.1.5",
  "license": "MIT",
  "exports": "./mod.ts",
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.0"
  },
  "tasks": {
    "test": "deno test --allow-env --allow-net",
    "fmt": "deno fmt",
    "lint": "deno lint",
    "check": "deno check mod.ts"
  },
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedIndexedAccess": true
  },
  "fmt": {
    "useTabs": false,
    "lineWidth": 100,
    "indentWidth": 2,
    "semiColons": true,
    "singleQuote": true,
    "proseWrap": "preserve",
    "include": ["src/", "mod.ts"],
    "exclude": ["dist/", "node_modules/"]
  },
  "lint": {
    "include": ["src/", "mod.ts"],
    "exclude": ["dist/", "node_modules/"],
    "rules": {
      "tags": ["recommended"]
    }
  }
}
